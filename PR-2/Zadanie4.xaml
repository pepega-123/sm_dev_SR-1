<Window x:Name="Zadanie_four" x:Class="PR_2.Zadanie4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR_2"
        mc:Ignorable="d"
        Title="Zadanie4" Height="400" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btn_back" Content="Назад" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" Click="btn_back_Click_1"/>
        <Label x:Name="l_nomer" Content="Задание 4" HorizontalAlignment="Center" Margin="0,55,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="l_textZadaniya" HorizontalAlignment="Center" Margin="0,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="21" Width="322" Text="Поменять местами элементы на 3 и 5 позициях"/>
        <Label x:Name="l_text2" Content="Входной массив: " HorizontalAlignment="Left" Margin="32,153,0,0" VerticalAlignment="Top"/>
        <Label x:Name="l_text3" Content="Выходной массив: " HorizontalAlignment="Left" Margin="32,192,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_generate" Content="Сгенерировать массив" HorizontalAlignment="Center" Margin="0,266,0,0" VerticalAlignment="Top" Click="btn_generate_Click"/>

    </Grid>
</Window>
